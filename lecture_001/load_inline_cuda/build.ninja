ninja_required_version = 1.3
cxx = c++
nvcc = /home/tornikeo/micromamba/envs/pb/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=square_matrix_extension -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include/TH -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include/THC -isystem /home/tornikeo/micromamba/envs/pb/include -isystem /home/tornikeo/micromamba/envs/pb/include/python3.10 -D_GLIBCXX_USE_CXX11_ABI=0 -fPIC -std=c++17
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=square_matrix_extension -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include/TH -isystem /home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/include/THC -isystem /home/tornikeo/micromamba/envs/pb/include -isystem /home/tornikeo/micromamba/envs/pb/include/python3.10 -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_61,code=sm_61 --compiler-options '-fPIC' -O2 -std=c++17
cuda_post_cflags = 
cuda_dlink_post_cflags = 
ldflags = -shared -L/home/tornikeo/micromamba/envs/pb/lib/python3.10/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda -ltorch -ltorch_python -L/home/tornikeo/micromamba/envs/pb/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags



rule link
  command = $cxx $in $ldflags -o $out

build main.o: compile /home/tornikeo/Documents/personal/forks/gpu-lectures/lecture_001/load_inline_cuda/main.cpp
build cuda.cuda.o: cuda_compile /home/tornikeo/Documents/personal/forks/gpu-lectures/lecture_001/load_inline_cuda/cuda.cu



build square_matrix_extension.so: link main.o cuda.cuda.o

default square_matrix_extension.so
